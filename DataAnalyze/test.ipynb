    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-11-01T12:03:56.339317400Z\",\n",
    "     \"start_time\": \"2025-11-01T12:02:18.002428Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"import requests\\n\",\n",
    "    \"import time\\n\",\n",
    "    \"import joblib  # 모델 로딩\\n\",\n",
    "    \"import numpy as np\\n\",\n",
    "    \"from datetime import datetime, timedelta, UTC\\n\",\n",
    "    \"from calendar import monthrange\\n\",\n",
    "    \"from geopy.distance import geodesic\\n\",\n",
    "    \"from geopy.point import Point\\n\",\n",
    "    \"# (tqdm은 실시간 예측에서는 필요 없으므로 제거)\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 1. ★★★ 설정 (중요) ★★★ ---\\n\",\n",
    "    \"# ⚠️ 여기에 Google API 키를 입력하세요 (이전 스크립트의 키는 작동하지 않습니다)\\n\",\n",
    "    \"GOOGLE_API_KEY = \\\"YOUR_GOOGLE_API_KEY_HERE\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 2. 모델링 파라미터 (우리가 분석한 결과) ---\\n\",\n",
    "    \"SLOPE_RADIUS_KM = 60\\n\",\n",
    "    \"SLOPE_THRESHOLD_METERS = 2000\\n\",\n",
    "    \"USGS_RADIUS_KM = 200\\n\",\n",
    "    \"USGS_MIN_MAGNITUDE = 6.0\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 3. 모델 및 스케일러 로드 (시작 시 1회) ---\\n\",\n",
    "    \"try:\\n\",\n",
    "    \"    model = joblib.load('tsunami_model.joblib')\\n\",\n",
    "    \"    scaler = joblib.load('tsunami_scaler.joblib')\\n\",\n",
    "    \"    print(\\\"[서비스 시작]  tsunami_model.joblib, tsunami_scaler.joblib 로드 성공.\\\")\\n\",\n",
    "    \"except FileNotFoundError:\\n\",\n",
    "    \"    print(\\\"⚠️ 치명적 오류: 'tsunami_model.joblib' 또는 'tsunami_scaler.joblib' 파일을 찾을 수 없습니다.\\\")\\n\",\n",
    "    \"    exit()\\n\",\n",
    "    \"\\n\",\n",
    "    \"# (추가) 이미 처리한 지진 ID를 저장 (중복 알림 방지)\\n\",\n",
    "    \"PROCESSED_QUAKE_IDS = set()\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 4. Elevation API 헬퍼 함수 (V16과 동일) ---\\n\",\n",
    "    \"def get_surrounding_points(lat, lon, radius_km):\\n\",\n",
    "    \"    center_point = Point(lat, lon)\\n\",\n",
    "    \"    points = {'center': (lat, lon)}\\n\",\n",
    "    \"    bearings = [0, 90, 180, 270] # 북, 동, 남, 서\\n\",\n",
    "    \"    names = ['north', 'east', 'south', 'west']\\n\",\n",
    "    \"    for name, bearing in zip(names, bearings):\\n\",\n",
    "    \"        destination = geodesic(kilometers=radius_km).destination(center_point, bearing)\\n\",\n",
    "    \"        points[name] = (destination.latitude, destination.longitude)\\n\",\n",
    "    \"    return points\\n\",\n",
    "    \"\\n\",\n",
    "    \"def get_elevation_features(lat, lon):\\n\",\n",
    "    \"    \\\"\\\"\\\"(특성 3, 4) is_ocean, is_steep_slope을 가져옵니다.\\\"\\\"\\\"\\n\",\n",
    "    \"    is_ocean = 0\\n\",\n",
    "    \"    is_steep_slope = 0\\n\",\n",
    "    \"\\n\",\n",
    "    \"    if GOOGLE_API_KEY == \\\"YOUR_GOOGLE_API_KEY_HERE\\\":\\n\",\n",
    "    \"        print(\\\"  [오류] Elevation API 키가 설정되지 않았습니다. (0, 0) 반환.\\\")\\n\",\n",
    "    \"        return 0, 0\\n\",\n",
    "    \"\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        points_to_check = get_surrounding_points(lat, lon, SLOPE_RADIUS_KM)\\n\",\n",
    "    \"        locations_list = [\\n\",\n",
    "    \"            points_to_check['center'], points_to_check['north'],\\n\",\n",
    "    \"            points_to_check['east'], points_to_check['south'], points_to_check['west']\\n\",\n",
    "    \"        ]\\n\",\n",
    "    \"        locations_str = \\\"|\\\".join([f\\\"{lt},{ln}\\\" for lt, ln in locations_list])\\n\",\n",
    "    \"\\n\",\n",
    "    \"        url = \\\"https://maps.googleapis.com/maps/api/elevation/json\\\"\\n\",\n",
    "    \"        params = {'locations': locations_str, 'key': GOOGLE_API_KEY}\\n\",\n",
    "    \"\\n\",\n",
    "    \"        response = requests.get(url, params=params, timeout=10)\\n\",\n",
    "    \"        response.raise_for_status()\\n\",\n",
    "    \"        data = response.json()\\n\",\n",
    "    \"\\n\",\n",
    "    \"        if data['status'] == 'OK':\\n\",\n",
    "    \"            results = data['results']\\n\",\n",
    "    \"            if not results: return 0, 0\\n\",\n",
    "    \"\\n\",\n",
    "    \"            center_elevation = results[0]['elevation']\\n\",\n",
    "    \"\\n\",\n",
    "    \"            # 1. (수정됨) is_ocean (바다인가?)\\n\",\n",
    "    \"            if center_elevation < 0:\\n\",\n",
    "    \"                is_ocean = 1\\n\",\n",
    "    \"\\n\",\n",
    "    \"            # 2. is_steep_slope (급경사인가?)\\n\",\n",
    "    \"            surrounding_elevations = [res['elevation'] for res in results[1:]]\\n\",\n",
    "    \"            for elev in surrounding_elevations:\\n\",\n",
    "    \"                if abs(center_elevation - elev) > SLOPE_THRESHOLD_METERS:\\n\",\n",
    "    \"                    is_steep_slope = 1\\n\",\n",
    "    \"                    break\\n\",\n",
    "    \"\\n\",\n",
    "    \"        return is_ocean, is_steep_slope\\n\",\n",
    "    \"\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        print(f\\\"  [오류] Elevation API 실패: {e}. (0, 0) 반환.\\\")\\n\",\n",
    "    \"        return 0, 0\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 5. USGS API 헬퍼 함수 (V14와 동일) ---\\n\",\n",
    "    \"def get_usgs_fault_counts(lat, lon):\\n\",\n",
    "    \"    \\\"\\\"\\\"(특성 5, 6) horizontal_count_1y_full, vertical_count_1y_full을 가져옵니다.\\\"\\\"\\\"\\n\",\n",
    "    \"    horizontal_count = 0\\n\",\n",
    "    \"    vertical_count = 0\\n\",\n",
    "    \"\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        # === 1단계: '검색' (과거 1년 ~ 현재 달 말일) ===\\n\",\n",
    "    \"        now = datetime.now(UTC) # UTC 기준으로 변경\\n\",\n",
    "    \"        year = now.year\\n\",\n",
    "    \"        month = now.month\\n\",\n",
    "    \"\\n\",\n",
    "    \"        _, last_day = monthrange(year, month)\\n\",\n",
    "    \"        endtime = f\\\"{year}-{month:02d}-{last_day}\\\"\\n\",\n",
    "    \"        starttime = f\\\"{year - 1}-{month:02d}-01\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"        search_url = \\\"https://earthquake.usgs.gov/fdsnws/event/1/query\\\"\\n\",\n",
    "    \"        params = {\\n\",\n",
    "    \"            'format': 'geojson',\\n\",\n",
    "    \"            'starttime': starttime,\\n\",\n",
    "    \"            'endtime': endtime,\\n\",\n",
    "    \"            'latitude': lat,\\n\",\n",
    "    \"            'longitude': lon,\\n\",\n",
    "    \"            'maxradiuskm': USGS_RADIUS_KM,\\n\",\n",
    "    \"            'minmagnitude': USGS_MIN_MAGNITUDE\\n\",\n",
    "    \"        }\\n\",\n",
    "    \"\\n\",\n",
    "    \"        response = requests.get(search_url, params=params, timeout=10)\\n\",\n",
    "    \"        response.raise_for_status()\\n\",\n",
    "    \"        data = response.json()\\n\",\n",
    "    \"\\n\",\n",
    "    \"        found_quakes = data.get('features', [])\\n\",\n",
    "    \"        if not found_quakes:\\n\",\n",
    "    \"            return 0, 0 # \\\"Not Found\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # === 2단계: '모든' 찾은 지진을 순회하며 'rake' 파싱 ===\\n\",\n",
    "    \"        for quake in found_quakes:\\n\",\n",
    "    \"            detail_url = quake['properties'].get('detail')\\n\",\n",
    "    \"            if not detail_url: continue\\n\",\n",
    "    \"\\n\",\n",
    "    \"            time.sleep(0.1) # 2차 접속 API 부하 방지\\n\",\n",
    "    \"            detail_response = requests.get(detail_url, timeout=10)\\n\",\n",
    "    \"            detail_response.raise_for_status()\\n\",\n",
    "    \"            detail_data = detail_response.json()\\n\",\n",
    "    \"\\n\",\n",
    "    \"            products = detail_data['properties'].get('products', {})\\n\",\n",
    "    \"            rake_value = None\\n\",\n",
    "    \"            all_products = products.get('moment-tensor', []) + products.get('focal-mechanism', [])\\n\",\n",
    "    \"            if not all_products: continue\\n\",\n",
    "    \"\\n\",\n",
    "    \"            best_product = None\\n\",\n",
    "    \"            for p in all_products:\\n\",\n",
    "    \"                if 'gcmt' in p.get('id','').lower(): best_product = p; break\\n\",\n",
    "    \"            if best_product is None:\\n\",\n",
    "    \"                for p in all_products:\\n\",\n",
    "    \"                     if 'us' in p.get('id','').lower() or p.get('code','').lower() == 'us': best_product = p; break\\n\",\n",
    "    \"            if best_product is None:\\n\",\n",
    "    \"                best_product = all_products[0]\\n\",\n",
    "    \"\\n\",\n",
    "    \"            if best_product:\\n\",\n",
    "    \"                props = best_product.get('properties', {})\\n\",\n",
    "    \"                rake_str = props.get('nodal-plane-1-rake')\\n\",\n",
    "    \"                if rake_str is None: rake_str = props.get('rake')\\n\",\n",
    "    \"                if rake_str is not None: rake_value = float(rake_str)\\n\",\n",
    "    \"\\n\",\n",
    "    \"            if rake_value is not None:\\n\",\n",
    "    \"                if (45 <= rake_value <= 135) or (-135 <= rake_value <= -45):\\n\",\n",
    "    \"                    vertical_count += 1\\n\",\n",
    "    \"                else:\\n\",\n",
    "    \"                    horizontal_count += 1\\n\",\n",
    "    \"\\n\",\n",
    "    \"        return horizontal_count, vertical_count\\n\",\n",
    "    \"\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        print(f\\\"  [오류] USGS API 실패: {e}. (0, 0) 반환.\\\")\\n\",\n",
    "    \"        return 0, 0\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 6. ★★★ 메인 예측 함수 (V16의 predict_tsunami) ★★★ ---\\n\",\n",
    "    \"def predict_tsunami(magnitude, depth, latitude, longitude):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    실시간 지진 데이터 4개를 입력받아 6개의 특성을 만들고 예측합니다.\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    print(f\\\"  [특성 수집 1/2] Elevation API 호출 (is_ocean, is_steep_slope)...\\\")\\n\",\n",
    "    \"    is_ocean, is_steep_slope = get_elevation_features(latitude, longitude)\\n\",\n",
    "    \"    print(f\\\"    -> 완료: is_ocean={is_ocean}, is_steep_slope={is_steep_slope}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    print(f\\\"  [특성 수집 2/2] USGS API 호출 (단층 카운트)...\\\")\\n\",\n",
    "    \"    h_count, v_count = get_usgs_fault_counts(latitude, longitude)\\n\",\n",
    "    \"    print(f\\\"    -> 완료: horizontal_count={h_count}, vertical_count={v_count}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # 6개 특성(Features) 조립\\n\",\n",
    "    \"    X_new = np.array([[\\n\",\n",
    "    \"        magnitude,\\n\",\n",
    "    \"        depth,\\n\",\n",
    "    \"        is_ocean,\\n\",\n",
    "    \"        is_steep_slope,\\n\",\n",
    "    \"        h_count,\\n\",\n",
    "    \"        v_count\\n\",\n",
    "    \"    ]])\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # (필수) 스케일링\\n\",\n",
    "    \"    X_scaled = scaler.transform(X_new)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # 예측\\n\",\n",
    "    \"    prediction = model.predict(X_scaled)[0]\\n\",\n",
    "    \"    probability = model.predict_proba(X_scaled)[0][1] # 1(쓰나미)일 확률\\n\",\n",
    "    \"\\n\",\n",
    "    \"    return prediction, probability\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 7. 실시간 지진 감시 메인 함수 (사용자님 코드 수정) ---\\n\",\n",
    "    \"def check_for_new_earthquakes():\\n\",\n",
    "    \"    # 지난 10분간 발생한 M 6.0 이상 '전 세계' 지진 검색\\n\",\n",
    "    \"    start_time = (datetime.now(UTC) - timedelta(minutes=10)).strftime('%Y-%m-%dT%H:%M:%S')\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # (수정) 사용자님의 지역 필터 제거 -> '전 세계' M 6.0+ 감시\\n\",\n",
    "    \"    params = {\\n\",\n",
    "    \"        'format': 'geojson',\\n\",\n",
    "    \"        'starttime': start_time,\\n\",\n",
    "    \"        'minmagnitude': 6.0\\n\",\n",
    "    \"    }\\n\",\n",
    "    \"    base_url = \\\"https://earthquake.usgs.gov/fdsnws/event/1/query\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        response = requests.get(base_url, params=params)\\n\",\n",
    "    \"        response.raise_for_status()\\n\",\n",
    "    \"        data = response.json()\\n\",\n",
    "    \"        count = data['metadata']['count']\\n\",\n",
    "    \"\\n\",\n",
    "    \"        if count > 0:\\n\",\n",
    "    \"            for feature in data['features']:\\n\",\n",
    "    \"                quake_id = feature['id']\\n\",\n",
    "    \"\\n\",\n",
    "    \"                # (수정) 이미 처리한 지진인지 확인\\n\",\n",
    "    \"                if quake_id in PROCESSED_QUAKE_IDS:\\n\",\n",
    "    \"                    continue # 이미 알림 보냈으면 건너뛰기\\n\",\n",
    "    \"\\n\",\n",
    "    \"                print(f\\\"\\\\n!!! [{datetime.now()}] 새 지진 감지 (ID: {quake_id}) !!!\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"                properties = feature['properties']\\n\",\n",
    "    \"                geometry = feature['geometry']['coordinates']\\n\",\n",
    "    \"\\n\",\n",
    "    \"                time_utc_obj = datetime.fromtimestamp(properties['time'] / 1000, UTC)\\n\",\n",
    "    \"                time_str = time_utc_obj.strftime('%Y-%m-%d %H:%M:%S (UTC)')\\n\",\n",
    "    \"\\n\",\n",
    "    \"                mag = properties['mag']\\n\",\n",
    "    \"                depth = geometry[2]\\n\",\n",
    "    \"                lat = geometry[1]\\n\",\n",
    "    \"                lon = geometry[0]\\n\",\n",
    "    \"                place = properties['place']\\n\",\n",
    "    \"\\n\",\n",
    "    \"                print(f\\\"  시간: {time_str}\\\")\\n\",\n",
    "    \"                print(f\\\"  위치: {place}\\\")\\n\",\n",
    "    \"                print(f\\\"  규모: {mag}, 깊이: {depth} km\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"                # --- (여기가 핵심) ---\\n\",\n",
    "    \"                # (수정) V16의 6개 특성 모델로 예측\\n\",\n",
    "    \"\\n\",\n",
    "    \"                prediction, probability = predict_tsunami(mag, depth, lat, lon)\\n\",\n",
    "    \"\\n\",\n",
    "    \"                if prediction == 1:\\n\",\n",
    "    \"                    print(\\\"\\\\n  [!!! 최종 경보: 쓰나미 발생 예측 !!!]\\\")\\n\",\n",
    "    \"                    print(f\\\"  이 지진의 쓰나미 발생 확률은 {probability * 100:.2f}% 입니다.\\\")\\n\",\n",
    "    \"                    # send_alert(\\\"!!! 쓰나미 경보 !!!\\\")\\n\",\n",
    "    \"                else:\\n\",\n",
    "    \"                    print(f\\\"\\\\n  [최종 분석: 안전]\\\")\\n\",\n",
    "    \"                    print(f\\\"  쓰나미 발생 확률 {probability * 100:.2f}%. 위험 낮음.\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"                # 이 지진은 처리 완료\\n\",\n",
    "    \"                PROCESSED_QUAKE_IDS.add(quake_id)\\n\",\n",
    "    \"                print(\\\"---\\\")\\n\",\n",
    "    \"        else:\\n\",\n",
    "    \"            print(f\\\"[{datetime.now(UTC).strftime('%Y-%m-%d %H:%M:%S')}] 지난 10분 내 M6.0+ 새 지진 없음.\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    except requests.exceptions.RequestException as e:\\n\",\n",
    "    \"        print(f\\\"API 호출 오류: {e}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 메인 코드: 60초마다 무한 반복 실행 ---\\n\",\n",
    "    \"if __name__ == \\\"__main__\\\":\\n\",\n",
    "    \"    if GOOGLE_API_KEY == \\\"YOUR_GOOGLE_API_KEY_HERE\\\":\\n\",\n",
    "    \"        print(\\\"=\\\"*50)\\n\",\n",
    "    \"        print(\\\"⚠️ 경고: 11번째 줄의 'GOOGLE_API_KEY'를 입력해야 스크립트가 작동합니다.\\\")\\n\",\n",
    "    \"        print(\\\"=\\\"*50)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    while True:\\n\",\n",
    "    \"        check_for_new_earthquakes()\\n\",\n",
    "    \"        time.sleep(60) # 60초(1분) 동안 대기\"\n",
    "   ],\n",
    "   \"id\": \"bc6f702c340fb83c\",\n",
    "   \"outputs\": [\n",
    "    {\n",
    "     \"name\": \"stdout\",\n",
    "     \"output_type\": \"stream\",\n",
    "     \"text\": [\n",
    "      \"[2025-11-01 21:02:18.812265] 지난 10분 내 새 지진 없음.\\n\",\n",
    "      \"[2025-11-01 21:03:19.600844] 지난 10분 내 새 지진 없음.\\n\"\n",
    "     ]\n",
    "    }\n",

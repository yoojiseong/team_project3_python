    "    }\n",
    "   },\n",
    "   \"source\": [\n",
    "    \"import requests\\n\",\n",
    "    \"import json\\n\",\n",
    "    \"from datetime import datetime, timedelta, UTC\\n\",\n",
    "    \"import time\\n\",\n",
    "    \"import math\\n\",\n",
    "    \"import joblib  # <-- [추가 1] joblib 파일 로드용\\n\",\n",
    "    \"import numpy as np # <-- [추가 2] 모델 입력 데이터 형식(배열)용\\n\",\n",
    "    \"\\n\",\n",
    "    \"# (이 라이브러리들은 성능 리포트에 필요합니다)\\n\",\n",
    "    \"from sklearn.metrics import confusion_matrix, recall_score, precision_score\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 1. Google Elevation API 키 ---\\n\",\n",
    "    \"# (키가 없으면 2차 검사는 건너뜁니다)\\n\",\n",
    "    \"GOOGLE_API_KEY = \\\"AIzaSyCQt1-_LLhTfX_0l6JAZU1WwlZ1ldkTVTw\\\" # <--- 여기에 실제 키를 넣어주세요\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 2. Google Elevation API 호출 함수 (지형 분석) ---\\n\",\n",
    "    \"def check_elevation_difference(epicenter_lat, epicenter_lon):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    진앙지 반경 60km 이내의 고도차(2km)를 확인합니다.\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    # (주의!) 키가 입력되었는지 확인하는 로직입니다. \\\"YOUR_API_KEY\\\"를 수정하시면 안 됩니다.\\n\",\n",
    "    \"    if GOOGLE_API_KEY == \\\"YOUR_API_KEY\\\":\\n\",\n",
    "    \"        # print(\\\"    [고도 분석 건너뜀] (Google API 키가 필요합니다)\\\")\\n\",\n",
    "    \"        return False # API 키가 없으면 실행 안 함\\n\",\n",
    "    \"\\n\",\n",
    "    \"    base_url = \\\"https://maps.googleapis.com/maps/api/elevation/json\\\"\\n\",\n",
    "    \"    delta = 0.54 # 60km\\n\",\n",
    "    \"    path = [\\n\",\n",
    "    \"        f\\\"{epicenter_lat + delta},{epicenter_lon}\\\", # 북\\n\",\n",
    "    \"        f\\\"{epicenter_lat - delta},{epicenter_lon}\\\", # 남\\n\",\n",
    "    \"        f\\\"{epicenter_lat},{epicenter_lon + delta}\\\", # 동\\n\",\n",
    "    \"        f\\\"{epicenter_lat},{epicenter_lon - delta}\\\", # 서\\n\",\n",
    "    \"        f\\\"{epicenter_lat},{epicenter_lon}\\\"          # 중앙\\n\",\n",
    "    \"    ]\\n\",\n",
    "    \"    params = { 'path': '|'.join(path), 'samples': 5, 'key': GOOGLE_API_KEY }\\n\",\n",
    "    \"\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        response = requests.get(base_url, params=params)\\n\",\n",
    "    \"        response.raise_for_status()\\n\",\n",
    "    \"        data = response.json()\\n\",\n",
    "    \"\\n\",\n",
    "    \"        if data['status'] == 'OK':\\n\",\n",
    "    \"            elevations = [result['elevation'] for result in data['results']]\\n\",\n",
    "    \"            difference = max(elevations) - min(elevations)\\n\",\n",
    "    \"\\n\",\n",
    "    \"            # (디버깅용) print(f\\\"    [고도 분석] 고도차: {difference:.2f} m\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"            if difference > 2000:\\n\",\n",
    "    \"                return True # 위험\\n\",\n",
    "    \"            else:\\n\",\n",
    "    \"                return False\\n\",\n",
    "    \"        else:\\n\",\n",
    "    \"            return False\\n\",\n",
    "    \"    except requests.exceptions.RequestException as e:\\n\",\n",
    "    \"        return False\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 3. (가짜) 모델: 우리가 만든 로직 ---\\n\",\n",
    "    \"def my_placeholder_model(mag, depth, lat, lon):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    [여기를 팀원의 모델로 교체하세요]\\n\",\n",
    "    \"    이것은 우리가 만든 임시 모델(Placeholder)입니다.\\n\",\n",
    "    \"    1. 규모 6.0 이상, 깊이 100km 미만인지 1차 확인\\n\",\n",
    "    \"    2. Google API로 2km 고도차 2차 확인\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    # 1차 경보 (규모 6.0 이상, 깊이 100km 미만)\\n\",\n",
    "    \"    if mag is not None and mag >= 6.0 and depth < 100.0:\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # 2차 경보 (Google API 호출)\\n\",\n",
    "    \"        if check_elevation_difference(lat, lon):\\n\",\n",
    "    \"            return 1 # (예측: 쓰나미 발생)\\n\",\n",
    "    \"        else:\\n\",\n",
    "    \"            return 0 # (예측: 쓰나미 미발생)\\n\",\n",
    "    \"    else:\\n\",\n",
    "    \"        return 0 # (예측: 쓰나미 미발생)\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 4. 시나리오(과거 데이터) 로더 (TypeError 수정된 버전) ---\\n\",\n",
    "    \"def load_historical_data(start_date=\\\"2020-01-01\\\",\\n\",\n",
    "    \"                         minlatitude=33, maxlatitude=43,\\n\",\n",
    "    \"                         minlongitude=128, maxlongitude=135):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    지정한 날짜부터 현재까지 '지정된 좌표'의 모든 지진 기록을 불러옵니다.\\n\",\n",
    "    \"    기본값은 '한국-일본 바다' 영역입니다.\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    print(f\\\"  > 로딩 조건: {start_date}부터, Lat({minlatitude}~{maxlatitude}), Lon({minlongitude}~{maxlongitude})\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    params = {\\n\",\n",
    "    \"        'format': 'geojson',\\n\",\n",
    "    \"        'starttime': start_date,\\n\",\n",
    "    \"        'minlatitude': minlatitude,   # 함수 인자로 받은 값을 사용\\n\",\n",
    "    \"        'maxlatitude': maxlatitude,   # 함수 인자로 받은 값을 사용\\n\",\n",
    "    \"        'minlongitude': minlongitude, # 함수 인자로 받은 값을 사용\\n\",\n",
    "    \"        'maxlongitude': maxlongitude  # 함수 인자로 받은 값을 사용\\n\",\n",
    "    \"    }\\n\",\n",
    "    \"    base_url = \\\"https://earthquake.usgs.gov/fdsnws/event/1/query\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        response = requests.get(base_url, params=params)\\n\",\n",
    "    \"        response.raise_for_status()\\n\",\n",
    "    \"        data = response.json()\\n\",\n",
    "    \"        return data['features'] # 모든 지진 이벤트 목록을 반환\\n\",\n",
    "    \"\\n\",\n",
    "    \"    except requests.exceptions.RequestException as e:\\n\",\n",
    "    \"        print(f\\\"과거 데이터 로딩 중 오류 발생: {e}\\\")\\n\",\n",
    "    \"        return []\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 5. 성능 리포트 출력 (IndexError 수정된 버전) ---\\n\",\n",
    "    \"def print_performance_report(y_true, y_pred):\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    '실제 정답'과 '모델 예측'을 비교하여 성능을 리포트합니다.\\n\",\n",
    "    \"    \\\"\\\"\\\"\\n\",\n",
    "    \"    # labels=[0, 1]을 추가하여, 쓰나미(1)가 없더라도 2x2 행렬을 강제로 생성\\n\",\n",
    "    \"    cm = confusion_matrix(y_true, y_pred, labels=[0, 1])\\n\",\n",
    "    \"\\n\",\n",
    "    \"    tn = cm[0][0] # (실제 X, 예측 X) - 잘 무시함\\n\",\n",
    "    \"    fp = cm[0][1] # (실제 X, 예측 O) - 잘못된 경보\\n\",\n",
    "    \"    fn = cm[1][0] # (실제 O, 예측 X) - 놓친 쓰나미 (가장 치명적)\\n\",\n",
    "    \"    tp = cm[1][1] # (실제 O, 예측 O) - 잘 맞춘 쓰나미\\n\",\n",
    "    \"\\n\",\n",
    "    \"    print(\\\"\\\\n--- [모델 성능 최종 리포트] ---\\\")\\n\",\n",
    "    \"    print(\\\"=== 혼동 행렬 (Confusion Matrix) ===\\\")\\n\",\n",
    "    \"    print(f\\\"                       (모델 예측)\\\")\\n\",\n",
    "    \"    print(f\\\"                   '위험 낮음' | '쓰나미 경보'\\\")\\n\",\n",
    "    \"    print(f\\\"(실제) 쓰나미 X  |     {tn:^6d} |     {fp:^6d}\\\")\\n\",\n",
    "    \"    print(f\\\"(실제) 쓰나미 O  |     {fn:^6d} |     {tp:^6d}\\\")\\n\",\n",
    "    \"    print(\\\"---------------------------------\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # 재현율 (Recall) 계산\\n\",\n",
    "    \"    recall = recall_score(y_true, y_pred, zero_division=0)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # 정밀도 (Precision) 계산\\n\",\n",
    "    \"    precision = precision_score(y_true, y_pred, zero_division=0)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    print(\\\"=== 핵심 성능 지표 ===\\\")\\n\",\n",
    "    \"    print(f\\\"  [재현율 (Recall)]   : {recall:.2%}\\\")\\n\",\n",
    "    \"    print(f\\\"    (실제 쓰나미 {fn+tp}건 중 {tp}건을 탐지 성공)\\\\n\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    print(f\\\"  [정밀도 (Precision)] : {precision:.2%}\\\")\\n\",\n",
    "    \"    print(f\\\"    (모델이 울린 총 {fp+tp}번의 경보 중 {tp}건이 실제 쓰나미)\\\\n\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    print(f\\\"  [총 '잘못된 경보'(False Positive)]: {fp} 건\\\")\\n\",\n",
    "    \"    print(f\\\"  [총 '놓친 쓰나미'(False Negative)]: {fn} 건  <-- (가장 위험)\\\")\\n\",\n",
    "    \"    print(\\\"---------------------------------\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# --- 6. 메인 시나리오 실행기 ---\\n\",\n",
    "    \"if __name__ == \\\"__main__\\\":\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # (1) 팀원의 모델로 교체하는 부분\\n\",\n",
    "    \"    #\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        # 1. 'tsunami_model.joblib' 파일 로드\\n\",\n",
    "    \"        real_model = joblib.load('tsunami_model.joblib')\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # 2. 'tsunami_model_to_test'에 '진짜 모델'을 할당합니다.\\n\",\n",
    "    \"        # 이 lambda 함수는 4개 변수를 받아 [1, 4] 형태의 2D 배열로 만들어 모델에 주입합니다.\\n\",\n",
    "    \"        tsunami_model_to_test = lambda mag, depth, lat, lon: real_model.predict(\\n\",\n",
    "    \"            np.array([[mag, depth, lat, lon]])\\n\",\n",
    "    \"        )[0]\\n\",\n",
    "    \"\\n\",\n",
    "    \"        model_name = \\\"real_model (tsunami_model.joblib)\\\"\\n\",\n",
    "    \"        print(\\\"--- [모델 로딩 성공] 'tsunami_model.joblib'를 사용합니다. ---\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    except FileNotFoundError:\\n\",\n",
    "    \"        print(\\\"--- [모델 로딩 실패] 'tsunami_model.joblib' 파일을 찾을 수 없습니다. ---\\\")\\n\",\n",
    "    \"        print(\\\"--- 임시 'my_placeholder_model'을 대신 사용합니다. ---\\\")\\n\",\n",
    "    \"        tsunami_model_to_test = my_placeholder_model\\n\",\n",
    "    \"        model_name = \\\"my_placeholder_model\\\"\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        print(f\\\"--- [모델 로딩 오류] {e} ---\\\")\\n\",\n",
    "    \"        print(\\\"--- 임시 'my_placeholder_model'을 대신 사용합니다. ---\\\")\\n\",\n",
    "    \"        tsunami_model_to_test = my_placeholder_model\\n\",\n",
    "    \"        model_name = \\\"my_placeholder_model\\\"\\n\",\n",
    "    \"    # -----------------------------------------------------------------\\n\",\n",
    "    \"\\n\",\n",
    "    \"    print(f\\\"\\\\n--- [시나리오 백테스트 시작] ---\\\")\\n\",\n",
    "    \"    print(f\\\"테스트할 모델: {model_name}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # A. 과거 데이터(시나리오) 로드\\n\",\n",
    "    \"    print(\\\"1. USGS에서 과거 지진 데이터(시나리오)를 불러옵니다 (2020년 1월 1일부터)...\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"    historical_events = load_historical_data(\\n\",\n",
    "    \"        start_date=\\\"2020-01-01\\\",\\n\",\n",
    "    \"        minlatitude=30,  # 일본 동쪽 해역\\n\",\n",
    "    \"        maxlatitude=45,\\n\",\n",
    "    \"        minlongitude=140,\\n\",\n",
    "    \"        maxlongitude=150\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    if not historical_events:\\n\",\n",
    "    \"        print(\\\"데이터 로딩 실패 또는 해당 기간 지진 없음.\\\")\\n\",\n",
    "    \"    else:\\n\",\n",
    "    \"        print(f\\\"총 {len(historical_events)}건의 과거 지진(시나리오) 확보.\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        y_true = [] # 실제 정답 (Real answers)\\n\",\n",
    "    \"        y_pred = [] # 모델 예측 (Model predictions)\\n\",\n",
    "    \"\\n\",\n",
    "    \"        print(\\\"2. 확보된 시나리오를 모델에 하나씩 입력하여 예측을 수행합니다...\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        for i, event in enumerate(historical_events):\\n\",\n",
    "    \"            # B. 시나리오 1건의 데이터 추출\\n\",\n",
    "    \"            props = event['properties']\\n\",\n",
    "    \"            geom = event['geometry']['coordinates']\\n\",\n",
    "    \"\\n\",\n",
    "    \"            mag = props['mag']\\n\",\n",
    "    \"            depth = geom[2]\\n\",\n",
    "    \"            lat = geom[1]\\n\",\n",
    "    \"            lon = geom[0]\\n\",\n",
    "    \"            place = props.get('place', 'Unknown Location') # 위치 정보가 없을 경우 대비\\n\",\n",
    "    \"\\n\",\n",
    "    \"            # C. '실제 정답' 추출\\n\",\n",
    "    \"            actual_tsunami = props['tsunami']\\n\",\n",
    "    \"            y_true.append(actual_tsunami)\\n\",\n",
    "    \"\\n\",\n",
    "    \"            # D. '모델 예측' 수행\\n\",\n",
    "    \"            try:\\n\",\n",
    "    \"                # 이 부분은 수정할 필요 없음:\\n\",\n",
    "    \"                # tsunami_model_to_test가 '진짜 모델'이든 '임시 모델'이든 동일하게 작동\\n\",\n",
    "    \"                predicted_tsunami = tsunami_model_to_test(mag, depth, lat, lon)\\n\",\n",
    "    \"            except Exception as e:\\n\",\n",
    "    \"                print(f\\\"  [모델 예측 오류] ({place}): {e}\\\")\\n\",\n",
    "    \"                print(f\\\"  > 입력값: mag={mag}, depth={depth}, lat={lat}, lon={lon}\\\")\\n\",\n",
    "    \"                predicted_tsunami = 0 # 오류 시 0 (안전)으로 예측\\n\",\n",
    "    \"\\n\",\n",
    "    \"            y_pred.append(predicted_tsunami)\\n\",\n",
    "    \"\\n\",\n",
    "    \"            if (i+1) % 200 == 0:\\n\",\n",
    "    \"                print(f\\\"  ... {i+1} / {len(historical_events)} 건 처리 중 ...\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        print(\\\"3. 예측 완료. 모델 성능(재현율)을 계산합니다...\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # E. 최종 성능 리포트 출력\\n\",\n",
    "    \"        print_performance_report(y_true, y_pred)\"\n",
    "   ],\n",
    "   \"outputs\": [\n",
    "    {\n",
    "     \"name\": \"stdout\",\n",
    "     \"output_type\": \"stream\",\n",
    "     \"text\": [\n",
    "      \"--- [모델 로딩 실패] 'tsunami_model.joblib' 파일을 찾을 수 없습니다. ---\\n\",\n",
    "      \"--- 임시 'my_placeholder_model'을 대신 사용합니다. ---\\n\",\n",
    "      \"\\n\",\n",
    "      \"--- [시나리오 백테스트 시작] ---\\n\",\n",
    "      \"테스트할 모델: my_placeholder_model\\n\",\n",
    "      \"1. USGS에서 과거 지진 데이터(시나리오)를 불러옵니다 (2020년 1월 1일부터)...\\n\",\n",
    "      \"  > 로딩 조건: 2020-01-01부터, Lat(30~45), Lon(140~150)\\n\",\n",
    "      \"총 3456건의 과거 지진(시나리오) 확보.\\n\",\n",
    "      \"2. 확보된 시나리오를 모델에 하나씩 입력하여 예측을 수행합니다...\\n\",\n",
    "      \"  ... 200 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 400 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 600 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 800 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 1000 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 1200 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 1400 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 1600 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 1800 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 2000 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 2200 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 2400 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 2600 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 2800 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 3000 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 3200 / 3456 건 처리 중 ...\\n\",\n",
    "      \"  ... 3400 / 3456 건 처리 중 ...\\n\",\n",
    "      \"3. 예측 완료. 모델 성능(재현율)을 계산합니다...\\n\",\n",
    "      \"\\n\",\n",
    "      \"--- [모델 성능 최종 리포트] ---\\n\",\n",
    "      \"=== 혼동 행렬 (Confusion Matrix) ===\\n\",\n",
    "      \"                       (모델 예측)\\n\",\n",
    "      \"                   '위험 낮음' | '쓰나미 경보'\\n\",\n",
    "      \"(실제) 쓰나미 X  |      3446  |       5   \\n\",\n",
    "      \"(실제) 쓰나미 O  |       4    |       1   \\n\",\n",
    "      \"---------------------------------\\n\",\n",
    "      \"=== 핵심 성능 지표 ===\\n\",\n",
    "      \"  [재현율 (Recall)]   : 20.00%\\n\",\n",
    "      \"    (실제 쓰나미 5건 중 1건을 탐지 성공)\\n\",\n",
    "      \"\\n\",\n",
    "      \"  [정밀도 (Precision)] : 16.67%\\n\",\n",
    "      \"    (모델이 울린 총 6번의 경보 중 1건이 실제 쓰나미)\\n\",\n",
    "      \"\\n\",\n",
    "      \"  [총 '잘못된 경보'(False Positive)]: 5 건\\n\",\n",
    "      \"  [총 '놓친 쓰나미'(False Negative)]: 4 건  <-- (가장 위험)\\n\",\n",
    "      \"---------------------------------\\n\"\n",
    "     ]\n",
    "    }\n",
    "   ],\n",
